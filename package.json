{
  "name": "filebump",
  "version": "0.0.2",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages-shared/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "dev:file-api": "npm run dev --workspace=packages/file-api",
    "dev:admin-backend": "npm run dev --workspace=packages/admin-backend",
    "dev:admin-ui": "npm run dev --workspace=packages/admin-ui",
    "dev:cron-jobs": "npm run dev --workspace=packages/cron-jobs",
    "build": "npm run build --workspaces",
    "build:admin-ui": "npm run build --workspace=packages/admin-ui",
    "build:admin": "npm run build:admin-ui && npm run start:admin-backend",
    "start:file-api": "npm run start --workspace=packages/file-api",
    "start:admin-backend": "npm run start --workspace=packages/admin-backend",
    "start:cron-jobs": "npm run start --workspace=packages/cron-jobs",
    "start:all": "npm run build:admin-ui && npm run start:file-api & npm run start:admin-backend & npm run start:cron-jobs & wait",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-fix": "eslint --fix .",
    "lint": "eslint ."
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "express-openapi": "^12.1.3",
    "form-data": "^4.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.6.0",
    "nanoid": "^3.3.7",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-no-floating-promise": "^1.0.2",
    "jest": "^29.7.0"
  }
}
