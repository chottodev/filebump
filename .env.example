# Root .env.example - Common variables for all packages

# Environment
NODE_ENV=development

# MongoDB (shared)
MONGODB_LOGS=mongodb://mongodb/filebumb-logs
MONGODB_DATA=mongodb://mongodb/filebump-files

# File API
FILE_API_PORT=3007
FILE_API_URL=http://localhost:3007
FILE_STORAGE_DIR=/tmp/uploads
FILE_API_KEYS=testKey1,testKey2
FILE_API_AUTH_HEADER=X-API-Key

# Admin Backend
ADMIN_BACKEND_PORT=33033
SESSION_SECRET=filebump-admin-secret
a
# Cron Jobs
CLEAR_FILES_TASK_SCHEDULE=0 0 1 * * *
